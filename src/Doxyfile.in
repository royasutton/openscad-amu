# src/Doxyfile
PROJECT_NAME           = "@PACKAGE_NAME@"
PROJECT_NUMBER         = "@PACKAGE_VERSION@"
PROJECT_BRIEF          =
OUTPUT_DIRECTORY       = doxygen
STRIP_FROM_PATH        = "@abs_top_srcdir@"
STRIP_FROM_INC_PATH    = "@abs_top_srcdir@"
JAVADOC_AUTOBRIEF      = NO
OPTIMIZE_OUTPUT_FOR_C  = NO
EXTENSION_MAPPING      = ll=c++
BUILTIN_STL_SUPPORT    = NO
DISTRIBUTE_GROUP_DOC   = NO
EXTRACT_PRIVATE        = YES
EXTRACT_PACKAGE        = NO
INTERNAL_DOCS          = NO
GENERATE_TODOLIST      = YES
GENERATE_TESTLIST      = YES
GENERATE_BUGLIST       = YES
GENERATE_DEPRECATEDLIST= YES
WARN_LOGFILE           = doxygen.warn
INPUT                  =
FILE_PATTERNS          = *.ll *.cpp *.hpp
RECURSIVE              = NO
EXAMPLE_PATH           = "@top_builddir@/share/examples" \
                         "@top_srcdir@/share/examples" \
                         "@builddir@"
EXAMPLE_RECURSIVE      = NO
IMAGE_PATH             =
INCLUDE_PATH           = "@top_builddir@"
FILTER_PATTERNS        =
GENERATE_HTML          = YES
HTML_OUTPUT            = html
GENERATE_TREEVIEW      = YES
GENERATE_LATEX         = YES
LATEX_OUTPUT           = latex
COMPACT_LATEX          = NO
PAPER_TYPE             = letter
GENERATE_MAN           = YES
MAN_OUTPUT             = man
PREDEFINED             = DOXYGEN_SHOULD_SKIP_THIS
GENERATE_TAGFILE       = doxygen.tagfile
TAGFILES               =
DOTFILE_DIRS           = "@top_srcdir@/share/examples"
